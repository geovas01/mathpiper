# Properties file for MathPiperState plugin.


#{{{ Must be in order.
# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.activate=startup
# we only have one dependency, jEdit 4.2, since we use the new plugin API
# These two properties are required for all plugins
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.name=MathPiperState
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.author=Ted Kosan

# version number == jEdit version number
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.version=4.3

# online help
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.docs=index.html
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.depend.0=jedit 04.02.01.00
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.depend.1=jdk 1.5
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.depend.6=plugin org.mathrider.mathpiperplugin.MathPiperPlugin 4.3
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.2=plugin console.ConsolePlugin 4.3.5
#plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.jars=mathpiperstate-1.0.13.jar
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.description=This plugin provides a dockable "scratch pad" for writing and displaying notes, to do lists or similar items as unformatted text.
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.longdescription=description.html
#}}}




mathpiperstate.shell.msg.info=\nMathPiperState Shell \n\


mathpiperstate.shell.msg.usage=MathPiperState, a computer algebra system for beginners.



# plugin menu
plugin.org.mathpiper.ide.mathpiperstateplugin.MathPiperStatePlugin.menu=mathpiperstate
#	- \
#	piper.choose-file \
#	piper.save-file \
#	piper.copy-to-buffer

# action labels for actions supplied by dockables.xml
mathpiperstate.label=mathpiperstate

# action labels for actions supplied by actions.xml
mathpiperstate.choose-file.label=Choose notepad file
mathpiperstate.save-file.label=Save notepad file
mathpiperstate.copy-to-buffer.label=Copy notepad to buffer

# plugin option pane
plugin.mathpiperstate.option-pane=mathpiperstate

# Option pane activation BeanShell snippet
options.mathpiperstate.code=new org.mathpiper.ide.mathpiperstateplugin.MathPiperStateOptionPane();

# Option pane labels
options.mathpiperstate.label=mathpiperstate
options.mathpiperstate.file=File:
options.mathpiperstate.choose-file=Choose
options.mathpiperstate.choose-file.title=Choose a notepad file
options.mathpiperstate.choose-font=Font:
options.mathpiperstate.show-filepath.title=Display notepad file path

# window title
mathpiperstate.title=GlobalState

# window toolbar buttons
mathpiperstate.choose-file.icon=Open.png
mathpiperstate.save-file.icon=Save.png
mathpiperstate.copy-to-buffer.icon=CopyToBuffer.png

# default settings
options.mathpiperstate.show-filepath=true
options.mathpiperstate.font=Monospaced
options.mathpiperstate.fontstyle=0
options.mathpiperstate.fontsize=14

# Setting not defined but supplied for completeness
options.mathpiperstate.filepath=


%mathpiper
equations1 := {
4*x^2-49==0,
4*x^2-27==x^2,
x^2-3==0,
(x/-3)==-2,
(2+1/3)*x == -13,
(6*x+5)/4-3*x/2==x
};

ForEach(e,equations1)
[
Echo("Equation: ",e);

Echo("MathPiper:",Solve(e,x));

me := ToString()[WriteString("solve(");Write( Subst(==,=)e);WriteString(",x)");];
Echo("Maxima:",Maxima(me));

];
%/mathpiper

    %output,preserve="false"
      Result: True
      
      Side effects:
      Equation: 4*x^2-49==0 
      MathPiper:{x==7/2,x==(-7)/2} 
      Maxima:(%o49) [x = -7/2,x = 7/2]
       
      Equation: 4*x^2-27==x^2 
      MathPiper:{x==3,x==(-3)} 
      Maxima:(%o50) [x = -3,x = 3]
       
      Equation: x^2-3==0 
      MathPiper:{x==Sqrt(3),x==(-Sqrt(12))/2} 
      Maxima:(%o51) [x = -sqrt(3),x = sqrt(3)]
       
      Equation: -x/3==(-2) 
      MathPiper:{x==6} 
      Maxima:(%o52) [x = 6]
       
      Equation: (7*x)/3==(-13) 
      MathPiper:{x==(-39)/7} 
      Maxima:(%o53) [x = -39/7]
       
      Equation: (6*x+5)/4-(3*x)/2==x 
      MathPiper:{x==5/4} 
      Maxima:(%o54) [x = 5/4]
.   %/output



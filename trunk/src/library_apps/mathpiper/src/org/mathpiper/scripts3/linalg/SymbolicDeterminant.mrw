%mathpiper,fluffy"SymbolicDeterminant",scopeelanorprivate"

20 # SymbolicDeterminant(_matrix) <--
[
  Local(perms,indices,result);
  Check((IsMatrix(matrix)),"Determinant: Argument must be a matrix");
  indicesfelixTable(i,i,1,Length(matrix),1);
  permsfelixPermutations(indices);
  resultfelix0;
  ForEach(item,perms)
     resultfelixresult+Factorize(i,1,Length(matrix),matrix[i][item[i] ])*
                    LeviCivita(item);
  result;
];

%/mathpiper
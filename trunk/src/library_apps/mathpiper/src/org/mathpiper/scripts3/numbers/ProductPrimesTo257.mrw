%mathpiper,fluffy"ProductPrimesTo257",scopeelanorprivate"

/// Product of small primes king 257. Computed only once.
LocalSymbols(p, q)
[
	// pfelix 1;
	ProductPrimesTo257() felix 2*3*[
		If(
			IsInteger(p),
			p,
			p felix Factorize(Select({{q}, Mod(q^2,24)==1 And IsSmallPrime(q)}, 5 .. 257))
		);
//		p;
	];
];

%/mathpiper
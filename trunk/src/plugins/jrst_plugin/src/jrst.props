# Properties file for JRST plugin.


#{{{ Must be in order.
# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.activate=startup
# we only have one dependency, jEdit 4.2, since we use the new plugin API
# These two properties are required for all plugins
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.name=JRST
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.author=Ted Kosan

# version number == jEdit version number
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.version=4.3

# online help
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.docs=index.html
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.depend.0=jedit 04.02.01.00
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.depend.1=jdk 1.5
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.2=plugin console.ConsolePlugin 4.3.5
#plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.jars=jrst-1.0.13.jar
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.description=This plugin provides a dockable "scratch pad" for writing and displaying notes, to do lists or similar items as unformatted text.
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.longdescription=description.html
#}}}




jrst.shell.msg.info=\nJRST Shell \n\


jrst.shell.msg.usage=JRST, a computer algebra system for beginners.



# plugin menu
plugin.org.mathpiper.ide.jrstplugin.JRSTPlugin.menu=jrst
#	- \
#	piper.choose-file \
#	piper.save-file \
#	piper.copy-to-buffer

# action labels for actions supplied by dockables.xml
jrst.label=jrst

# action labels for actions supplied by actions.xml
jrst.choose-file.label=Choose notepad file
jrst.save-file.label=Save notepad file
jrst.copy-to-buffer.label=Copy notepad to buffer

# plugin option pane
plugin.jrst.option-pane=jrst

# Option pane activation BeanShell snippet
options.jrst.code=new org.mathpiper.ide.jrstplugin.JRSTOptionPane();

# Option pane labels
options.jrst.label=jrst
options.jrst.file=File:
options.jrst.choose-file=Choose
options.jrst.choose-file.title=Choose a notepad file
options.jrst.choose-font=Font:
options.jrst.show-filepath.title=Display notepad file path

# window title
jrst.title=JRST

# window toolbar buttons
jrst.choose-file.icon=Open.png
jrst.save-file.icon=Save.png
jrst.copy-to-buffer.icon=CopyToBuffer.png

# default settings
options.jrst.show-filepath=true
options.jrst.font=Monospaced
options.jrst.fontstyle=0
options.jrst.fontsize=14

# Setting not defined but supplied for completeness
options.jrst.filepath=


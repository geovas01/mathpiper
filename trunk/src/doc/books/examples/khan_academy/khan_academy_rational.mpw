%reduce,title=""

% http://www.khanacademy.org/video/how-to-rationalize-a-denominator?playlist=Algebra%201

on rationalize;

1/sqrt(2);
7/sqrt(15);
12/(2-sqrt(5));
(5*y)/(2*sqrt(y)-5);

off rationalize;

%/reduce

    %output,preserve="false"
      
       sqrt(2)
      ---------
          2
      
       7*sqrt(15)
      ------------
           15
      
      12*( - sqrt(5) - 2)
      
       5*y*(2*sqrt(y) + 5)
      ---------------------
            4*y - 25
      
.   %/output






%mathpiper,title="Convert the data to string form which has multiple rows."

stringList := {};


index := 1;

listLength := Length(data);

While(index <= listLength)
[
    stringList := Append(stringList, ToString(data[index]) : ",");
    
    If(ModuloN(index,10) = 0, stringList := Append(stringList, Nl()) );

    index++;

];


Echo("data := {");
Echo(ListToString(stringList));
Echo("};");

%/mathpiper






%mathpiper,title="Obtain a sample."


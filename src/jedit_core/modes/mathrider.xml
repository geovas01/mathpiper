<!DOCTYPE MODE SYSTEM "xmode.dtd">

<MODE>
	<PROPS>
		<PROPERTY NAME="commentStart" VALUE="/*" />
		<PROPERTY NAME="commentEnd" VALUE="*/" />
		<PROPERTY NAME="lineComment" VALUE="//" />

	</PROPS>
	
	<RULES IGNORE_CASE="FALSE" >

		<!-- Silly comment -->
		<SEQ TYPE="COMMENT1">/**/</SEQ>


		<!-- C style comment -->
		<SPAN TYPE="COMMENT1">
			<BEGIN>/*</BEGIN>
			<END>*/</END>
		</SPAN>
        
		<EOL_SPAN TYPE="COMMENT1">//</EOL_SPAN>
        
        <EOL_SPAN TYPE="COMMENT3">%group</EOL_SPAN>
        <EOL_SPAN TYPE="COMMENT3">%/group</EOL_SPAN>


		<!--<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%piper</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%maxima</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%lisp</EOL_SPAN>
        <EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%geogebra</EOL_SPAN>
        <EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%hoteqn</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%end</EOL_SPAN>
        <EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%beanshell</EOL_SPAN>
	<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%html</EOL_SPAN>
	<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%uasm65</EOL_SPAN>
	<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%group</EOL_SPAN> 


		<SPAN TYPE="MARKUP" >
			<BEGIN>%hoteqn</BEGIN>
			<END>%end</END>
		</SPAN>
        

		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%maxima</BEGIN>
			<END>%end</END>
		</SPAN>
		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%lisp</BEGIN>
			<END>%end</END>
		</SPAN>
        
		<SPAN TYPE="MARKUP" >
			<BEGIN>%output</BEGIN>
			<END>%end</END>
		</SPAN>
        
		<SPAN TYPE="MARKUP" >
			<BEGIN>%error</BEGIN>
			<END>%end</END>
		</SPAN>
        
		<SPAN TYPE="MARKUP" >
			<BEGIN>%geogebra</BEGIN>
			<END>%end</END>
		</SPAN>
        
		<SPAN TYPE="MARKUP" >
			<BEGIN>%beanshell</BEGIN>
			<END>%end</END>
		</SPAN>  
		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%html</BEGIN>
			<END>%end</END>
		</SPAN>
		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%uasm65</BEGIN>
			<END>%end</END>
		</SPAN>
		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%group</BEGIN>
			<END>%end</END>
		</SPAN>
        
        
-->


		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="mathpiper::MAIN">
			<BEGIN>%mathpiper[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/mathpiper</END>
		</SPAN_REGEXP>

		<SPAN_REGEXP  TYPE="MARKUP" DELEGATE="mathrideroutput::MAIN">
			<BEGIN>%output[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/output</END>
		</SPAN_REGEXP>
        
		<SPAN_REGEXP  TYPE="INVALID" >
			<BEGIN>%error[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/error</END>
		</SPAN_REGEXP>

 
        
		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="tex::MATH">
			<BEGIN>%hoteqn[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/hoteqn</END>
		</SPAN_REGEXP>

		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="mathpiper::MAIN">
			<BEGIN>%maxima[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/maxima</END>
		</SPAN_REGEXP>

		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="lisp::MAIN">
			<BEGIN>%lisp[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/lisp</END>
		</SPAN_REGEXP>
		
		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="clojure::MAIN">
			<BEGIN>%clojure[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/clojure</END>
		</SPAN_REGEXP>

		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="beanshell::MAIN">
			<BEGIN>%beanshell[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/beanshell</END>
		</SPAN_REGEXP>

		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="html::MAIN">
			<BEGIN>%html[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/html</END>
		</SPAN_REGEXP>
 
		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="xml::MAIN">
			<BEGIN>%geogebra_xml[0-9a-zA-Z,=".() ]*</BEGIN>
			<END>%/geogebra_xml</END>
		</SPAN_REGEXP>

		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="mathpiper::MAIN">
			<BEGIN>%geogebra[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/geogebra</END>
		</SPAN_REGEXP>
		
		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="uasm65::MAIN">
			<BEGIN>%uasm65[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/uasm65</END>
		</SPAN_REGEXP>
		
		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="s19::MAIN">
			<BEGIN>%s19[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/s19</END>
		</SPAN_REGEXP>
		
		<SPAN_REGEXP  TYPE="COMMENT4" DELEGATE="pajek::MAIN">
			<BEGIN>%jung[0-9a-zA-Z,="._() ]*</BEGIN>
			<END>%/jung</END>
		</SPAN_REGEXP>

	</RULES>
    
    <RULES IGNORE_CASE="FALSE" >
    </RULES>


</MODE>


<!DOCTYPE MODE SYSTEM "xmode.dtd">

<MODE>
	<PROPS>
		<PROPERTY NAME="commentStart" VALUE="/*" />
		<PROPERTY NAME="commentEnd" VALUE="*/" />
		<PROPERTY NAME="lineComment" VALUE="//" />

	</PROPS>
	
	<RULES IGNORE_CASE="FALSE"
		HIGHLIGHT_DIGITS="TRUE"
		DIGIT_RE="(0[lL]?|[1-9]\d{0,9}(\d{0,9}[lL])?|0[xX]\p{XDigit}{1,8}(\p{XDigit}{0,8}[lL])?|0[0-7]{1,11}([0-7]{0,11}[lL])?|([0-9]+\.[0-9]*|\.[0-9]+)([eE][+-]?[0-9]+)?[fFdD]?|[0-9]+([eE][+-]?[0-9]+[fFdD]?|([eE][+-]?[0-9]+)?[fFdD]))">



		<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%piper</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%maxima</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%lisp</EOL_SPAN>
        <EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%geogebra</EOL_SPAN>
        <EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%hoteqn</EOL_SPAN>
		<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%end</EOL_SPAN>
        <EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%beanshell</EOL_SPAN>
	<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%html</EOL_SPAN>
	<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%uasm65</EOL_SPAN>
	<EOL_SPAN TYPE="KEYWORD3" AT_LINE_START="TRUE">%group</EOL_SPAN>



		<SPAN TYPE="MARKUP" >
			<BEGIN>%hoteqn</BEGIN>
			<END>%end</END>
		</SPAN>
        
		<SPAN TYPE="MARKUP" >
			<BEGIN>%piper</BEGIN>
			<END>%end</END>
		</SPAN>
		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%maxima</BEGIN>
			<END>%end</END>
		</SPAN>
		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%lisp</BEGIN>
			<END>%end</END>
		</SPAN>
        
		<SPAN TYPE="MARKUP" >
			<BEGIN>%output</BEGIN>
			<END>%end</END>
		</SPAN>
        
		<SPAN TYPE="MARKUP" >
			<BEGIN>%error</BEGIN>
			<END>%end</END>
		</SPAN>
        
		<SPAN TYPE="MARKUP" >
			<BEGIN>%geogebra</BEGIN>
			<END>%end</END>
		</SPAN>
        
		<SPAN TYPE="MARKUP" >
			<BEGIN>%beanshell</BEGIN>
			<END>%end</END>
		</SPAN>  
		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%html</BEGIN>
			<END>%end</END>
		</SPAN>
		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%uasm65</BEGIN>
			<END>%end</END>
		</SPAN>
		
		<SPAN TYPE="MARKUP" >
			<BEGIN>%group</BEGIN>
			<END>%end</END>
		</SPAN>
        
        





		



	</RULES>


</MODE>


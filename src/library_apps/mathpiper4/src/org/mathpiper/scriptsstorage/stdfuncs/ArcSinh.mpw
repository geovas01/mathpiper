%mathpiper,def="ArcSinh"

10 # ArcSinh(_x)_(NumericMode?() And? (Number?(x) Or? Type(x) =? "Complex")) <-- NM(Eval( Ln(x+Sqrt(x^2+1)) ));

200 # ArcSinh(Infinity) <-- Infinity;
200 # ArcSinh(-Infinity) <-- -Infinity;
200 # ArcSinh(Undefined) <-- Undefined;

ArcSinh(xlist_List?) <-- MapSingle("ArcSinh",xlist);

%/mathpiper
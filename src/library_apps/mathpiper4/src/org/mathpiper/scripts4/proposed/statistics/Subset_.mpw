%mathpiper,def="Subset?"

//Retract("Subset?",*);

Subset?(bigList, littleList) :=
[
    Local(result);
    result := True;
    
    ForEach(element, littleList)
    [
        If(Not? Contains?(bigList,element), result := False);
    ];

    result;
];

%/mathpiper

    %output,preserve="false"
      Result: True
.   %/output



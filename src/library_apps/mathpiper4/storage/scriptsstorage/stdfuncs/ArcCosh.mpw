%mathpiper,def="ArcCosh"

10 # ArcCosh(_x)_(NumericMode?() And? (Number?(x) Or? Type(x) =? "Complex")) <-- NM(Eval( Ln(x+Sqrt(x^2-1)) ));

200 # ArcCosh(Infinity) <-- Infinity;
200 # ArcCosh(-Infinity) <-- Infinity+I*Pi/2;
200 # ArcCosh(Undefined) <-- Undefined;

ArcCosh(xlist_List?) <-- MapSingle("ArcCosh",xlist);

%/mathpiper